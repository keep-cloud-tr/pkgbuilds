script=keep-wg
pkgname=scripts-$script
pkgver=0.0.1
pkgrel=1
pkgdesc="Set up keep network."
arch=('any')
license=('MIT')
depends=(
  wireguard-tools
  openssh
)
optdepends=()
source=(
  "$script"
)
sha256sums=(
  'SKIP'
)

package() {
  install -Dm755 "$srcdir/$script" "$pkgdir/usr/local/bin/$script"
}
